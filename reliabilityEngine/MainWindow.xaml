<Window x:Class="reliabilityEngine.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:reliabilityEngine"
        mc:Ignorable="d"
        Title="Reliability Block Diagram Engine" Height="450" Width="800">
    <Grid Background="Aqua">
        <DockPanel Name="treeDock" LastChildFill="False" Margin="5,25,650,5">
            <TreeView Name="mainTree" Width="150" Background="Beige"/>
        </DockPanel>
        <DockPanel LastChildFill="False" Margin="155,25,5,5" Background="Coral">
            <WrapPanel Name="rbdPanel" Width="645" Background="AliceBlue">

            </WrapPanel>

        </DockPanel>
        <Button Name="Add" Content="Button" HorizontalAlignment="Left" Margin="509,0,0,0" VerticalAlignment="Top" Click="Add_Click"/>
    </Grid>
</Window>
